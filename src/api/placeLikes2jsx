// /api/placeLikes.jsx

import axios from "axios";

const API_SERVER_HOST = "http://localhost:8081";
const prefixSearch = `${API_SERVER_HOST}/search`;

export const getOne = async (contentid) => {
  const res = await axios.get(`${prefixSearch}/detail`, {
    params: { contentid },
  });
  return res.data;
};
